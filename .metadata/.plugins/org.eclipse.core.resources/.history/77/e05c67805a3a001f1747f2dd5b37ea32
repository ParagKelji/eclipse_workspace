package leetcode;

public class MajorityElement {

	public int majorityElement(int[] nums) {
		int count = 0;
		if ( nums == null || nums.length <= 0 ) {
			return 0;
		}
		int [] counts = new int[nums.length];
		for ( int i = 0 ; i < nums.length; i++ ) {
			int num = nums[i];
			counts[num] += 1;
			count = counts[num];
		}
		
		return count;
	}
	
	public static void main(String[] args) {
		
	}

}
